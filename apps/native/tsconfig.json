{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/components/*": ["./components/*"],
      "@/providers/*": ["./utils/providers/*"],
      "@/stores/*": ["./utils/stores/*"],
      "@/utils/*": ["./utils/*"],
      "@/api/*": ["./../api/src/*"]
    }
  },
  "include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts"]
}
